var array : int [20];

func main () {

    var i : int = 0;
    var counter : int = 0;
    while (i < 10) {
        array[i] = counter;
        i = i + 1;
        counter = counter + 1;
    }
    while (i > 0) {
        array[i] = counter;
        i = i + 1;
        counter = counter - 1;
    }
    print("────────────────────────────");
    i = 0;
    while (i < 20) {
        print(array[i]);
        i = i + 1; 
    }
    print(i);
    print("---");
    print(array);

    chart(array);
    return 0;
}